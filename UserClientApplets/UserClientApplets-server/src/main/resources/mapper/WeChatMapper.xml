<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.shmashine.userclientapplets.dao.WeChatLoginDao">

    <!--用户电梯批量授权-->
    <insert id="addUserResource">
        insert into tbl_sys_user_resource(v_user_id, v_resource_id, dt_create_time, v_create_user_id) values
        <foreach collection="elevatorIds" item="elevatorId" separator=",">
            (#{userId},#{elevatorId},NOW(),#{requestUserId})
        </foreach>
    </insert>

</mapper>