<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shmashine.pm.api.dao.TblGroupLeasingStatisticsMapper">
    <sql id="TABLE_NAME">
        tbl_group_leasing_statistics
    </sql>
    <sql id="BASE_COLUMNS">
        village_id
        , elevator_code, statistics_type, floor, day_coefficient, average_coefficient, percent, `level`

    </sql>
    <sql id="ALL_COLUMNS">
        <include refid="BASE_COLUMNS"/>,
        id, create_time, modify_time
    </sql>
    <sql id="SET_COLUMNS">
        <if test="entity.dayCoefficient != null">day_coefficient = #{entity.dayCoefficient},</if>
        <if test="entity.averageCoefficient != null">average_coefficient = #{entity.averageCoefficient},</if>
        <if test="entity.percent != null">percent = #{entity.percent},</if>
        <if test="entity.level != null">level = #{entity.level},</if>
    </sql>

    <!-- 查询 -->

    <select id="getById" resultType="com.shmashine.common.entity.TblGroupLeasingStatisticsEntity">
        SELECT
        <include refid="ALL_COLUMNS"/>
        FROM
        <include refid="TABLE_NAME"/>
        WHERE id = #{id}
    </select>

    <select id="getByUnique" resultType="com.shmashine.common.entity.TblGroupLeasingStatisticsEntity">
        SELECT
        <include refid="ALL_COLUMNS"/>
        FROM
        <include refid="TABLE_NAME"/>
        <where>
            <if test="entity.id != null">
                AND id = #{entity.id}
            </if>
            <if test="entity.elevatorCode != null and entity.elevatorCode != ''">
                AND elevator_code = #{entity.elevatorCode}
            </if>
            <if test="entity.statisticsType != null and entity.statisticsType != ''">
                AND statistics_type = #{entity.statisticsType}
            </if>
            <if test="entity.floor != null">
                AND floor = #{entity.floor}
            </if>
        </where>
    </select>

    <select id="findByEntity" resultType="com.shmashine.common.entity.TblGroupLeasingStatisticsEntity">
        SELECT
        <include refid="ALL_COLUMNS"/>
        FROM
        <include refid="TABLE_NAME"/>
        <where>
            <if test="entity.id != null">
                AND id = #{entity.id}
            </if>
            <if test="entity.elevatorCode != null and entity.elevatorCode != ''">
                AND elevator_code = #{entity.elevatorCode}
            </if>
            <if test="entity.statisticsType != null and entity.statisticsType != ''">
                AND statistics_type = #{entity.statisticsType}
            </if>
            <if test="entity.floor != null">
                AND floor = #{entity.floor}
            </if>
            <if test="entity.villageId != null and entity.villageId != ''">
                AND village_id = #{entity.villageId}
            </if>
            <if test="entity.level != null">
                AND `level` &gt;= #{entity.level}
            </if>
        </where>
    </select>

    <!-- 添加 -->
    <insert id="insert" keyProperty="id" useGeneratedKeys="true"
            parameterType="com.shmashine.common.entity.TblGroupLeasingStatisticsEntity">
        INSERT IGNORE INTO
        <include refid="TABLE_NAME"/>
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="entity.elevatorCode != null and entity.elevatorCode != ''">elevator_code,</if>
            <if test="entity.statisticsType != null and entity.statisticsType != ''">statistics_type,</if>
            <if test="entity.floor != null and entity.floor != ''">floor,</if>
            <if test="entity.villageId != null and entity.villageId != ''">village_id,</if>
            <if test="entity.dayCoefficient != null">day_coefficient,</if>
            <if test="entity.averageCoefficient != null">average_coefficient,</if>
            <if test="entity.percent != null">percent,</if>
            <if test="entity.level != null">level,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="entity.elevatorCode != null and entity.elevatorCode != ''">#{entity.elevatorCode},</if>
            <if test="entity.statisticsType != null and entity.statisticsType != ''">#{entity.statisticsType},</if>
            <if test="entity.floor != null and entity.floor != ''">#{entity.floor},</if>
            <if test="entity.villageId != null and entity.villageId != ''">#{entity.villageId},</if>
            <if test="entity.dayCoefficient != null">#{entity.dayCoefficient},</if>
            <if test="entity.averageCoefficient != null">#{entity.averageCoefficient},</if>
            <if test="entity.percent != null">#{entity.percent},</if>
            <if test="entity.level != null">#{entity.level},</if>
        </trim>
    </insert>

    <!-- 删除 -->
    <delete id="deleteById">
        DELETE
        FROM
        <include refid="TABLE_NAME"/>
        WHERE id = #{id}
    </delete>

    <!-- 更新 -->
    <update id="updateById" parameterType="com.shmashine.common.entity.TblGroupLeasingStatisticsEntity">
        UPDATE
        <include refid="TABLE_NAME"/>
        <set>
            <if test="entity.elevatorCode != null and entity.elevatorCode != ''">elevator_code =
                #{entity.elevatorCode},
            </if>
            <if test="entity.statisticsType != null and entity.statisticsType != ''">statistics_type =
                #{entity.statisticsType},
            </if>
            <if test="entity.floor != null and entity.floor != ''">floor = #{entity.floor},</if>
            <if test="entity.villageId != null and entity.villageId != ''">village_id = #{entity.villageId},</if>
            <include refid="SET_COLUMNS"/>
        </set>
        WHERE id = #{entity.id}
    </update>


</mapper>