    <select id="list" parameterType="com.shmashine.api.module.mashineService.MashineServiceBizElevatorModule"
            resultMap="MashineServiceBizElevatorResult">
        select tbl_third_party_ruijin_elevator.* from tbl_third_party_ruijin_elevator
        join tbl_elevator on tbl_third_party_ruijin_elevator.register_number = tbl_elevator.v_equipment_code
        <if test="!mashineServiceBizElevatorModule.isAdminFlag">
            join tbl_sys_user_resource resource on tbl_elevator.v_elevator_id = resource.v_resource_id and
            resource.v_user_id =
            #{mashineServiceBizElevatorModule.userId}
        </if>
        order by tbl_third_party_ruijin_elevator.id desc
    </select>
