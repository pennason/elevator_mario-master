<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shmashine.api.dao.TblGroupLeasingFloorEvidenceMapper">

    <sql id="TABLE_NAME">
        tbl_group_leasing_floor_evidence
    </sql>
    <sql id="BASE_COLUMNS">
        elevator_code
        , floor, start_time, end_time, `status`
    </sql>
    <sql id="ALL_COLUMNS">
        <include refid="BASE_COLUMNS"/>,
        id, create_time, modify_time
    </sql>
    <sql id="SET_COLUMNS">
        <if test="entity.startTime != null">start_time = #{entity.startTime},</if>
        <if test="entity.endTime != null">end_time = #{entity.endTime},</if>
        <if test="entity.status != null">status = #{entity.status},</if>
    </sql>
    <!-- 查询 -->

    <select id="getById" resultType="com.shmashine.common.entity.TblGroupLeasingFloorEvidenceEntity">
        SELECT
        <include refid="ALL_COLUMNS"/>
        FROM
        <include refid="TABLE_NAME"/>
        WHERE id = #{id}
    </select>

    <select id="findByEntity" resultType="com.shmashine.common.entity.TblGroupLeasingFloorEvidenceEntity">
        SELECT
        <include refid="ALL_COLUMNS"/>
        FROM
        <include refid="TABLE_NAME"/>
        <where>
            <if test="entity.id != null">
                AND id = #{entity.id}
            </if>
            <if test="entity.elevatorCode != null and entity.elevatorCode != ''">
                AND elevator_code = #{entity.elevatorCode}
            </if>
            <if test="entity.floor != null and entity.floor != ''">
                AND floor = #{entity.floor}
            </if>
        </where>
    </select>

    <select id="getByUnique" resultType="com.shmashine.common.entity.TblGroupLeasingFloorEvidenceEntity">
        SELECT
        <include refid="ALL_COLUMNS"/>
        FROM
        <include refid="TABLE_NAME"/>
        WHERE elevator_code = #{elevatorCode} AND floor = #{floor}
    </select>

    <!-- 添加 -->
    <insert id="insert" keyProperty="id" useGeneratedKeys="true"
            parameterType="com.shmashine.common.entity.TblGroupLeasingFloorEvidenceEntity">
        INSERT IGNORE INTO
        <include refid="TABLE_NAME"/>
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="entity.elevatorCode != null and entity.elevatorCode != ''">elevator_code,</if>
            <if test="entity.floor != null and entity.floor != ''">floor,</if>
            <if test="entity.startTime != null">start_time,</if>
            <if test="entity.endTime != null">end_time,</if>
            <if test="entity.status != null">status,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="entity.elevatorCode != null and entity.elevatorCode != ''">#{entity.elevatorCode},</if>
            <if test="entity.floor != null and entity.floor != ''">#{entity.floor},</if>
            <if test="entity.startTime != null">#{entity.startTime},</if>
            <if test="entity.endTime != null">#{entity.endTime},</if>
            <if test="entity.status != null">#{entity.status},</if>
        </trim>
    </insert>

    <insert id="save" keyProperty="id" useGeneratedKeys="true"
            parameterType="com.shmashine.common.entity.TblGroupLeasingFloorEvidenceEntity">
        INSERT INTO
        <include refid="TABLE_NAME"/>
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="entity.elevatorCode != null and entity.elevatorCode != ''">elevator_code,</if>
            <if test="entity.floor != null and entity.floor != ''">floor,</if>
            <if test="entity.startTime != null">start_time,</if>
            <if test="entity.endTime != null">end_time,</if>
            <if test="entity.status != null">status,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="entity.elevatorCode != null and entity.elevatorCode != ''">#{entity.elevatorCode},</if>
            <if test="entity.floor != null and entity.floor != ''">#{entity.floor},</if>
            <if test="entity.startTime != null">#{entity.startTime},</if>
            <if test="entity.endTime != null">#{entity.endTime},</if>
            <if test="entity.status != null">#{entity.status},</if>
        </trim>
        ON DUPLICATE KEY UPDATE
        start_time = #{entity.startTime}, end_time = #{entity.endTime}, status = #{entity.status}
    </insert>

    <!-- 删除 -->
    <delete id="deleteById">
        DELETE
        FROM
        <include refid="TABLE_NAME"/>
        WHERE id = #{id}
    </delete>

    <!-- 更新 -->
    <update id="updateById" parameterType="com.shmashine.common.entity.TblGroupLeasingFloorEvidenceEntity">
        UPDATE
        <include refid="TABLE_NAME"/>
        <set>
            <if test="entity.elevatorCode != null and entity.elevatorCode != ''">elevator_code =
                #{entity.elevatorCode},
            </if>
            <if test="entity.floor != null and entity.floor != ''">floor = #{entity.floor},</if>
            <include refid="SET_COLUMNS"/>
        </set>
        WHERE id = #{entity.id}
    </update>
</mapper>
